#include <stdio.h>
#include <string.h>

int main() {
    const char *haystack = "Hello, World!";
    const char *needle = "World";

    // Using strstr to find the substring
    char *result = strstr(haystack, needle);

    if (result != NULL) {
        printf("Found substring: %s\n", result);  // Prints "World!"
    } else {
        printf("Substring not found\n");
    }

    return 0;
}
