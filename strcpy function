#include <stdio.h>
#include <string.h>

int main() {
    char source[] = "Hello, World!";
    char destination[50];

    // Using strcpy to copy the string
    strcpy(destination, source);

    // Printing the destination string
    printf("Source: %s\n", source);
    printf("Destination: %s\n", destination);

    return 0;
}
